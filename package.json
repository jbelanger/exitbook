{
  "name": "exitbook",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "crypto",
    "transactions",
    "import"
  ],
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "blockchain-config:generate": "pnpm -F @exitbook/blockchain-providers blockchain-config:generate",
    "blockchain-config:validate": "pnpm -F @exitbook/blockchain-providers blockchain-config:validate",
    "blockchain-providers:list": "pnpm -F @exitbook/blockchain-providers blockchain-providers:list",
    "blockchain-providers:validate": "pnpm -F @exitbook/blockchain-providers blockchain-providers:validate",
    "build": "pnpm -r build",
    "clean": "pnpm -r run clean",
    "dev": "pnpm -F exitbook-cli dev",
    "postinstall": "node scripts/fix-libsodium.mjs",
    "lint": "pnpm -r run lint",
    "prepare": "husky",
    "prettier": "pnpm -r run prettier",
    "prettier:fix": "pnpm -r run prettier:fix",
    "providers:list": "pnpm -F @exitbook/blockchain-providers providers:list",
    "providers:sync": "pnpm -F @exitbook/blockchain-providers providers:sync",
    "providers:validate": "pnpm -F @exitbook/blockchain-providers providers:validate",
    "start": "pnpm -F exitbook-cli start",
    "test": "node --env-file-if-exists .env $(which pnpm) -r run test",
    "test:coverage": "node tools/run-vitest-coverage.mjs",
    "test:e2e": "node --env-file-if-exists .env node_modules/vitest/vitest.mjs run --config vitest.e2e.config.ts"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,yaml,yml}": [
      "prettier --write"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@effect/eslint-plugin": "^0.3.2",
    "@eslint/js": "^9.37.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "baseline-browser-mapping": "^2.9.17",
    "eslint": "^9.37.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "engines": {
    "node": ">=23.0.0",
    "pnpm": ">=10.6.2"
  }
}
